{
  "name": "@bluelibs/apollo-bundle",
  "version": "2.0.1",
  "description": "Zero-config Apollo/GraphQL solution for BlueLibs Framework",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/bluelibs"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "test": "jest --verbose dist/__tests__/index.js",
    "test:clean": "jest --clearCache",
    "test:dev": "jest --verbose --watch src/__tests__/index.ts --detectOpenHandles",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "gen-doc": "typedoc ./src/index.ts --exclude ./src/__tests__ --nulout typeDocs --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "@as-integrations/aws-lambda": "^2.0.1",
    "@graphql-tools/schema": "^8.3.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "fs-capacitor": "^8.0.0",
    "graphql-jit": "^0.8.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^16.0.2",
    "graphql-ws": "^5.12.1",
    "subscriptions-transport-ws": "^0.9.19",
    "tiny-lru": "^7.0.6"
  },
  "peerDependencies": {
    "@bluelibs/core": "^1.0.2",
    "@bluelibs/graphql-bundle": "^2.0.0",
    "graphql": "^16.0.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.11",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@bluelibs/core": "^1.0.2",
    "@bluelibs/email-bundle": "^1.0.1",
    "@bluelibs/graphql-bundle": "^2.0.0",
    "@bluelibs/logger-bundle": "^1.1.0",
    "@bluelibs/security-bundle": "^1.0.4",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.3",
    "babel-jest": "^29.5.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "graphql": "^16.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "mocha": "^9.1.3",
    "plugin": "^0.0.15",
    "prettier": "^2.4.1",
    "serverless": "^3.7.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.1",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.7",
    "typescript": "^4.8.0",
    "ws": "^8.2.3"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
