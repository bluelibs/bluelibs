{
  "name": "@bluelibs/x-ui",
  "version": "1.0.2",
  "private": false,
  "description": "X-UI represents a set of standards to build scalable UI using X-Framework by BlueLibs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/bluelibs"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "test": "jest --verbose dist/__tests__/index.js",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "gen-doc": "typedoc ./src/index.ts --exclude ./src/__tests__ --out typeDocs --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@bluelibs/ejson": "^1.0.0",
    "@types/mongodb": "^3.6.3",
    "apollo-upload-client": "^16.0.0",
    "history": "^4.10.1",
    "json-to-graphql-query": "^2.0.0",
    "node-polyglot": "^2.4.0",
    "query-string": "^6.13.2",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "peerDependencies": {
    "@apollo/client": "^3.2.1",
    "@bluelibs/core": "^1.0.0",
    "@bluelibs/ejson": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.1",
    "@bluelibs/core": "^1.0.0",
    "@bluelibs/ejson": "^1.0.0",
    "@bluelibs/smart": "^1.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.170",
    "@types/node": "^8.0.28",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^17.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-fetch": "^3.0.6",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "graphql": "^15.3.0",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1",
    "source-map-support": "^0.5.13",
    "typescript": "^4.0.2"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
